<?xml version="1.0" encoding="utf-8"?>
<shape xmlns:android="http://schemas.android.com/apk/res/android"
       android:shape="ring"

       android:innerRadiusRatio="10"

       android:thicknessRatio="4"
       android:useLevel="false"
    > <!-- android:innerRadius="20dp" android:thickness="5dp" -->
    <gradient
        android:angle="45"
        android:centerColor="#FF0000FF"
        android:centerX="@dimen/centerX"
        android:centerY="@dimen/centerY"
        android:endColor="#80FF00FF"
        android:gradientRadius="@dimen/gradient_radius"
        android:startColor="#FFFF0000"
        android:type="radial"
        android:useLevel="false"
        />

    <corners
        android:radius="8.3dp"
        android:topLeftRadius="3.3dp"
        android:topRightRadius="4.3dp"
        android:bottomRightRadius="5.3dp"
        android:bottomLeftRadius="6.3dp"
        />

    <padding
        android:left="14.3dp"
        android:top="15.3dp"
        android:right="16.3dp"
        android:bottom="17.3dp"
        />
    <size
        android:width="80.3dp"
        android:height="50.3dp"
        />
    <!--
        <solid
            android:color="#FF0000"
            />
    -->
    <stroke
        android:width="1.3dp"
        android:color="@android:color/black"
        android:dashWidth="0.3dp"
        android:dashGap="0.3dp"
        />
</shape>
